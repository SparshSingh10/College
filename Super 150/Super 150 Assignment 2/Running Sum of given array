import java.util.*;
public class Main {
    public static void main (String args[]) {
   Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for(int i = 0; i<arr.length;i++){
            arr[i] = sc.nextInt();
        }
        int s=0;
        for(int i=0;i<arr.length;i++){
            s=s+arr[i];
            System.out.print(s+" ");
        }
    }
}
